{
  "name": "require-watch",
  "version": "2.1.0",
  "description": "Automatically refresh require dependencies in NodeJS",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/*",
    "compile": "rm -rf dist/; babel --plugins @babel/transform-runtime,add-module-exports -d dist/ src/",
    "prepublish": "npm test && npm run compile",
    "test": "npm run lint; BABEL_ENV=test NODE_DEBUG=require-watch nyc tape test/**/*.js | tap-spec",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Benjamin Goh &lt;masotime@gmail.com&gt;",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/masotime/require-watch.git"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime-corejs2": "^7.0.0",
    "chokidar": "^2.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "blue-tape": "^1.0.0",
    "bluebird": "^3.5.5",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.3",
    "nyc": "^14.1.1",
    "shortid": "^2.2.14",
    "tap-spec": "^5.0.0"
  }
}
